<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>UpdateVegetableServlet</servlet-name>
        <servlet-class>servlets.UpdateVegetableServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateVegetableServlet</servlet-name>
        <url-pattern>/update/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteVegetableServlet</servlet-name>
        <servlet-class>servlets.DeleteVegetableServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteVegetableServlet</servlet-name>
        <url-pattern>/delete/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SellVegetableServlet</servlet-name>
        <servlet-class>servlets.SellVegetableServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SellVegetableServlet</servlet-name>
        <url-pattern>/sell/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>VegetableServlet</servlet-name>
        <servlet-class>servlets.VegetableServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VegetableServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
</web-app>
